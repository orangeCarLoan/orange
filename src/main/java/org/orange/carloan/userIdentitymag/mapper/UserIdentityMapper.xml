<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.orange.carloan.userIdentitymag.mapper.UserIdentityMapper">
	<select id="findUserIdentityByUserIdentityId" parameterType="int" resultMap="resMap">
		SELECT * FROM t_useridentity WHERE id =#{id}
	</select>
	<resultMap type="org.orange.carloan.beans.UserIdentityBean" id="resMap">
		<id property="id" column="id"></id>
		<result property="userName" column="username"></result>
		<result property="type" column="type"></result>
		<result property="address" column="address"></result>
		<result property="birthday" column="birthday"></result>
		<result property="gender" column="gender"></result>
		<result property="identity" column="identity"></result>
		<result property="nation" column="nation"></result>
		<result property="organ" column="organ"></result>
		<result property="stratDate" column="stratDate"></result>
		<result property="endDate" column="endDate"></result>
		<association property="contractInformationBean" javaType="ContractInformationBean"
			column="id" select="org.orange.carloan.contractinformationmag.mapper.ContractInformationMapper.findContractInformationByUserIdentityId">
		</association>
	</resultMap>
	<select id="findUserIdentityByIdentity" parameterType="string" resultMap="resMap">
		SELECT * FROM t_useridentity WHERE identity=#{identity}
	</select>
</mapper>